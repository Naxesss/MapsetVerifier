{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "tauri-app",
  "version": "2.0.0",
  "identifier": "net.greaper.mapset-verifier",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Mapset Verifier 2.0.0",
        "width": 800,
        "height": 600,
        "minWidth": 800,
        "minHeight":600,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/64x64.png",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "fs": {
      "requireLiteralLeadingDot": false
    },
    "opener": {}
  }
}
